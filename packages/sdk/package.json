    "lint": "tsc --noEmit"
  },
  "keywords": [
    "surfpool",
    "solana",
    "rpc",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.95.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
